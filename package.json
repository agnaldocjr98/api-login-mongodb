{
   "name": "tsapimongo",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/bcrypt": "^5.0.0",
      "@types/body-parser": "^1.19.1",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/jsonwebtoken": "^8.5.4",
      "@types/module-alias": "^2.0.1",
      "@types/mongodb": "^4.0.7",
      "@types/node": "^16.3.1",
      "@types/restify-cors-middleware": "^1.0.2",
      "@typescript-eslint/eslint-plugin": "^5.8.1",
      "@typescript-eslint/parser": "^5.8.1",
      "eslint": "^7.2.0",
      "eslint-config-airbnb-base": "^15.0.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-import-resolver-typescript": "^2.5.0",
      "eslint-plugin-import": "^2.25.3",
      "eslint-plugin-import-helpers": "^1.2.0",
      "eslint-plugin-prettier": "^4.0.0",
      "git-commit-msg-linter": "^4.1.1",
      "husky": "^4.3.8",
      "ts-node-dev": "^1.1.8",
      "tsconfig-paths": "^3.10.1",
      "typescript": "^4.3.5"
   },
   "dependencies": {
      "axios": "^0.21.1",
      "bcrypt": "^5.0.1",
      "cors": "^2.8.5",
      "dotenv": "^16.0.1",
      "express": "^4.17.1",
      "jsonwebtoken": "^8.5.1",
      "module-alias": "^2.2.2",
      "moment": "^2.29.3",
      "mongodb": "^4.7.0",
      "node-gyp": "^8.1.0",
      "prettier": "^2.5.1"
   },
   "scripts": {
      "dev": "ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpile-only --ignore-watch node_modules --no-notify ./src/main/server.ts",
      "build": "tsc -p .",
      "clean": "tsc --build --clean",
      "start": "npm run build && node ./build/src/main/server.js",
      "gitcommit": "set NODE_ENV=GITCOMMIT && npm run build && node ./build/index.js && set NODE_ENV=DEVELOPMENT",
      "up-dev": "git pull origin develop && yarn build && pm2 restart \"api-edmil\"",
      "up-prod": "git pull -f origin master && yarn build && pm2 restart \"api-edmil\"",
      "down": "pm2 stop \"api-edmil\""
   }
}
